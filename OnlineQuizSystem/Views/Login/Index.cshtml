
@{
    ViewBag.Title = "Index";
}

<h2>Sign In</h2>
<form method="post">
   
    <input type="text" name="Name" value="@Model.Name" class="form-control" placeholder="User Name" />
    <input type="password" name="Password" value="@Model.Password" class="form-control" placeholder="Password" />
    <input type="submit" value="LogIn" class="btn btn-success" />
</form>

